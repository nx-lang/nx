// Function definition with properties
import core.html

let <Button
  text:string
  disabled:bool=false
/>: Element =
  <button
    class="btn"
    disabled={disabled}
  >
    {text}
  </button>

// Paren-style function with explicit return type
let add(a:int, b:int): int = { a + b }

// Paren-style function without explicit return type
let subtract(a:int, b:int) = { a - b }
