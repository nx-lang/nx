// Various expression types
let <MathDemo
  x:int=10
  y:int=20
/> =
  <div>
    <p:>Sum: {x + y}</p>
    <p:>Product: {x * y}</p>
    <p:>Remainder: {y % 3}</p>
    <p:>Comparison: {x < y ? "x is less" : "x is greater or equal"}</p>
    <p:>Logical: {x > 0 && y > 0}</p>
    <p:>Negated Comparison: {! (x > y)}</p>
  </div>
