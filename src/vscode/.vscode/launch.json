{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run NX Language Extension",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": [
        "--remote=wsl+${env:WSL_DISTRO_NAME}",
        "--extensionDevelopmentPath=${workspaceFolder}/src/vscode"
      ]
    }
  ]
}
